function [y_out] = time_series_output(y_out,time_out,rad_energy,rad_pressure);
%TIME_SERIES_OUTPUT Uncomment plots which are supposed to print out line
%plot values every interval timesteps, including the first one
persistent out_count;

if isempty(out_count)
    outcount = 0;
end
out_count = out_count+1; 

%Jiang14 Figure 2
y_out(out_count,1) = rad_flux(nx/2,ny/2,1);
%advective flux
y_out(out_count,2) = v(nx/2,ny/2)*(rad_energy(nx/2,ny/2) + rad_pressure(nx/2,ny/2,1,1))/C; 

%Is there a way for the function to see the entire workspace rather than
%pass in each variable manually?
plot(time_plot(1:out_count),y_out(1:out_count,1),'-o',time_plot(1:out_count),y_out(1:out_count,2),'-o');





     %Figure 1
%     y_out(out_count) = rad_energy(nx/2,ny/2);
%     y_out2(out_count) = temp(nx/2,ny/2)^4;
% % %     figure(1);
%       legend('E_r','T^4');

%      xlabel('time (s)');
%      ylabel('E_r,T^4');
%      title('\sigma_a = 1');


end

